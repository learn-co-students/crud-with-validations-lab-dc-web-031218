<h1>Create a Song!</h1>
<div>
<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %></br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %></br>
  <%= f.label :genre %>
  <%= f.text_field :genre %></br>
  <%= f.label :release_year %>
  <%= f.text_field :release_year %></br>
  <%= f.label :released %>
  <%= f.text_field :released %></br>

  <%= f.submit%>
<% end %>
</div>





