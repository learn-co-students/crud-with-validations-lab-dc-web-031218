<h1><%= @song.title %></h1>
<div>
<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %></br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %></br>
  <%= f.label :genre %>
  <%= f.text_field :genre %></br>
  <%= f.label :release_year %>
  <%= f.text_field :release_year %></br>
  <%= f.label :released %>
  <%= f.text_field :released %></br>
  <%= link_to "Edit This Song", edit_song(@song) %>
  <%= f.submit%>
<% end %>
</div>
<%= link_to "Delete Song", song_path(@song), method: "delete" %>





