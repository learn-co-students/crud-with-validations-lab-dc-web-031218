<table>
  <tr>
    <th>Title</th>
    <th>Released?</th>
    <th>Release Year</th>
    <th>Artist Name</th>
    <th>Genre</th>
  </tr>

<tr>

  <td><%= @song.title %></td>
  <td><%= @song.released ? "Yes" : "No"%></td>
  <td><%= @song.release_year %></td>
  <td><%= @song.artist_name %></td>
  <td><%= @song.genre %></td>
  <td><%= link_to 'Edit Song', edit_song_path(@song) %></td>
  <td><%= link_to 'Delete Song', @song, method: :delete, data: {confirm: "Are you sure you want to delete \"" + @song.title + "\"?"} %></td>
  <td></td>
</tr>
</table>
